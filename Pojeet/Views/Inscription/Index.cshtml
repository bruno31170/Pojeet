@model List<Pojeet.Models.CompteConsumer>
@{
    Layout = "_Layout";
    ViewBag.Title = "AjouterConsumer";
}



<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Nom</th>
            <th scope="col">Prenom</th>
            <th scope="col">Mail</th>
            <th scope="col">Action</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>@item.Profil.Nom</td>
                <td>@item.Profil.Prenom</td>
                <td>@item.Profil.Mail</td>
                <td>@Html.ActionLink("Modifier", "ModifierConsumer", "Inscription", new { @id = item.Id })</td>
            </tr>



        }
       
    </tbody>
</table>
